{
  "name": "diminish",
  "version": "0.6.0",
  "engines": {
    "node": ">=6.4.0"
  },
  "description": "Minimal dependency injection to diminish code complexity",
  "main": "src",
  "types": "src",
  "author": "Asa Jones <thinkinfractals@gmail.com> (https://github.com/codechaotic)",
  "license": "ISC",
  "keywords": [
    "container",
    "typescript",
    "dependency",
    "injection",
    "inversion",
    "control",
    "ioc",
    "di"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/codechaotic/diminish"
  },
  "bugs": {
    "url": "https://github.com/codechaotic/diminish/issues"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "src/**/*.d.ts*",
      "src/**/*.spec.ts"
    ]
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ]
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf src/**/*.d.ts* src/**/*.js* coverage",
    "lint": "standard src/**/*.ts",
    "coverage": "nyc report --reporter=html && opn ./coverage/index.html",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc mocha --opts mocha.opts src/**/*.spec.ts"
  },
  "dependencies": {
    "esprima": "^4.0.1",
    "glob": "^7.1.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/esprima": "^4.0.2",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.14.4",
    "@types/sinon": "^7.0.11",
    "@types/sinon-chai": "^3.2.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.3",
    "eslint-plugin-typescript": "^0.14.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.3.0",
    "opn-cli": "^4.0.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.1",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.11",
    "standard": "^12.0.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.4.1",
    "typescript-eslint-parser": "^21.0.2"
  }
}
