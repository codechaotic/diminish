{
	"author": "Asa Jones <thinkinfractals@gmail.com> (https://github.com/codechaotic)",
	"bugs": {
		"url": "https://github.com/codechaotic/diminish/issues"
	},
	"dependencies": {
		"@types/glob": "^7.1.1",
		"esprima": "^4.0.1",
		"glob": "^7.1.3"
	},
	"description": "Minimal dependency injection to diminish code complexity",
	"devDependencies": {
		"@types/chai": "^4.2.6",
		"@types/chai-as-promised": "^7.1.2",
		"@types/esprima": "^4.0.2",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.17.6",
		"@types/sinon": "^7.5.1",
		"@types/sinon-chai": "^3.2.3",
		"chai": "^4.2.0",
		"chai-as-promised": "^7.1.1",
		"coveralls": "^3.0.9",
		"mocha": "^7.0.0",
		"mocha-lcov-reporter": "^1.3.0",
		"nyc": "^15.0.0",
		"open-cli": "^5.0.0",
		"rimraf": "^3.0.0",
		"sinon": "^7.5.0",
		"sinon-chai": "^3.3.0",
		"source-map-support": "^0.5.16",
		"ts-node": "^8.5.4",
		"tslint": "^5.20.1",
		"tslint-config-standard": "^9.0.0",
		"typescript": "^3.7.5"
	},
	"keywords": [
		"container",
		"typescript",
		"dependency",
		"injection",
		"inversion",
		"control",
		"ioc",
		"di"
	],
	"license": "ISC",
	"main": "dist",
	"name": "diminish",
	"nyc": {
		"extension": [
			".ts",
			".tsx"
		],
		"exclude": [
			"src/**/*.d.ts*",
			"src/**/*.spec.ts"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/codechaotic/diminish"
	},
	"scripts": {
		"build": "rimraf dest types && tsc",
		"clean": "rimraf dest types",
		"lint": "tslint --project tsconfig.json src/**/*.ts",
		"coverage": "nyc report --reporter=html && open-cli ./coverage/index.html",
		"report-coverage": "nyc report --reporter=text-lcov | coveralls",
		"test": "nyc mocha --config mocharc.yml src/**/*.spec.ts"
	},
	"types": "types",
	"version": "1.0.0"
}
